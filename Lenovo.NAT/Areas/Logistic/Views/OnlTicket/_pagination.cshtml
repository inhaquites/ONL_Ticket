@using Lenovo.NAT.ViewModel.Logistic.OnlTicket
@model OnlTicketFilter

Total of @Model.TotalItems

<nav aria-label="Page navigation">
    <ul class="pagination justify-content-end">
        <li class="page-item" style="margin-left: 10px;">
            <nav aria-label="Page navigation">
                <ul class="pagination justify-content-end">
                    <li class="page-item disabled">
                        <a class="page-link" href="#">
                            Items per page
                        </a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" id="Items10" asp-area="Logistic" asp-controller="OnlTicket" asp-action="Index" asp-route-pageSize=10 aria-label="10">10</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" id="Items25" asp-area="Logistic" asp-controller="OnlTicket" asp-action="Index" asp-route-pageSize=25 aria-label="25">25</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" id="Items50" asp-area="Logistic" asp-controller="OnlTicket" asp-action="Index" asp-route-pageSize=50 aria-label="50">50</a>
                    </li>
                    <li class="page-item">
                        <a class="page-link" id="Items100" asp-area="Logistic" asp-controller="OnlTicket" asp-action="Index" asp-route-pageSize=100 aria-label="100">100</a>
                    </li>
                </ul>
            </nav>
        </li>

        <li class="page-item" style="margin-left: 10px;">
            <nav aria-label="Page navigation">
                <ul class="pagination justify-content-end">
                    <li class="page-item">
                        <a class="page-link @Model.Previous"
                           id="Previous"
                           asp-area="Logistic"
                           asp-controller="OnlTicket"
                           asp-action="Index"
                           asp-route-page="@(Model.Page - 1)"
                           asp-route-pageSize="@Model.PageSize"
                           asp-route-logNumber="@Model.LogNumber"
                           asp-route-customer="@Model.Customer"
                           asp-route-segment="@Model.Segment"
                           asp-route-orderType="@Model.OrderType"
                           asp-route-assignTo="@Model.AssignTo"
                           asp-route-agingBucket="@Model.AgingBucket"
                           asp-route-createdAt="@Model.CreatedAt"
                           asp-route-createdBy="@Model.CreatedBy"
                           asp-route-status="@Model.Status"
                           asp-route-sapOrder="@Model.SAPOrder"
                           aria-label="Previous">
                            Previous
                        </a>
                    </li>
                    <li class="page-item disabled">
                        <a class="page-link" href="#">
                            Page @(Model.Page + 1) of @Model.TotalPages
                        </a>
                    </li>
                    <li class="page-item">
                        <a class="page-link @Model.Next"
                           id="Next"
                           asp-area="Logistic"
                           asp-controller="OnlTicket"
                           asp-action="Index"
                           asp-route-page="@(Model.Page + 1)"
                           asp-route-pageSize="@Model.PageSize"
                           asp-route-logNumber="@Model.LogNumber"
                           asp-route-customer="@Model.Customer"
                           asp-route-segment="@Model.Segment"
                           asp-route-orderType="@Model.OrderType"
                           asp-route-assignTo="@Model.AssignTo"
                           asp-route-agingBucket="@Model.AgingBucket"
                           asp-route-createdAt="@Model.CreatedAt"
                           asp-route-createdBy="@Model.CreatedBy"
                           asp-route-status="@Model.Status"
                           asp-route-sapOrder="@Model.SAPOrder"
                           aria-label="Next">
                            Next
                        </a>
                    </li>
                </ul>
            </nav>
        </li>
    </ul>
</nav>
